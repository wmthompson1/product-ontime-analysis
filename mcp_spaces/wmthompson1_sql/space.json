{
  "name": "wmthompson1_sql",
  "version": "0.1.0",
  "type": "command",
  "description": "Local, dev-only SQL assistant Space (runs with local Python, no Docker)",
  "command": ["bash", "-lc", "./run-local.sh"],
  "cwd": "./",
  "ports": [8080],
  "inputs": [
    {
      "id": "AUTH_TOKEN",
      "promptString": "Authorization token for the Space (optional)",
      "password": true
    },
    {
      "id": "DB_PATH",
      "promptString": "Path to SQLite DB (optional)",
      "password": false
    }
  ]
}
